{
    "model": "SVDPoseModel",
    "data_dir": "/workspace/raid/krb/oxford-radar-robotcar-dataset/",
    "log_dir": "/workspace/Documents/logs/2021-01-10/",
    "gpuid": "cuda:0",
    "min_range": 58,
    "radar_resolution": 0.0432,
    "cart_resolution": 0.3456,
    "cart_pixel_width": 640,
    "input_channels": 1,
    "train_split": [0, 24],
    "validation_split": [24, 25],
    "test_split": [3, 4],
    "networks": {
        "unet": {
            "bilinear": true,
            "first_feature_dimension": 8,
            "score_sigmoid": true
        },
        "keypoint_block": {
            "patch_size": 32,
            "softmax_temp": 1.0,
            "mask_threshold": 0.05
        },
        "matcher_block": {
            "softmax_temp": 0.01,
            "score_comp": true
        }
    },
    "lr": 0.0001,
    "weight_decay": 0.0001,
    "clip_norm": 1.0,
    "batch_size": 2,
    "window_size": 2,
    "num_workers": 4,
    "max_iterations": 250000,
    "max_epochs": 3,
    "print_rate": 10,
    "log_rate": 10,
    "save_rate": 5000,
    "val_rate": 5000,
    "vis_num": 5,
    "vis_keypoint_nms": 0.01,
    "augmentation": {
        "augment": true,
        "rot_max": 0.19634954084936207
    },
    "loss": "pointmatch_loss"
}
